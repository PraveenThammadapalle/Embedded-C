#include <stdio.h>
 
void main()
{
    char str[50];
    int i, length = 0;
 
    printf("Enter the string: \n");
    gets(str);

    for (i = 0; str[i] != '\0'; i++)
    {
        length++;
    }
    printf("The length of a string is the number of characters in it \n");
    printf("So, the length of %s = %d\n", str, length);
}
