#include <stdio.h>

int main() {
    unsigned int a = 5; // binary: 0101
    unsigned int b = 3; // binary: 0011
    
    // Bitwise operations
    printf("a & b = %u\n", a & b);   // 1
    printf("a | b = %u\n", a | b);       // 7
    printf("a ^ b = %u\n", a ^ b);     // 6
    printf("~a = %u\n", ~a);               // 4294967290 (in 32-bit unsigned)
    printf("a << 1 = %u\n", a << 1);   // 10
    printf("a >> 1 = %u\n", a >> 1);     // 2

    return 0;
}
