///Constant Pointer////
#include<stdio.h>
     int main()
     {
        int const *p = 5;
        int q;
        p = &q;
            printf("%d",++(*p)); 
     }


////Constant int -- constant pointer//////
 #include<stdio.h>
     int main()
     {
         int x = 10;
         int const *const p;
         p = &x;
         printf("%d\n", *p);
     }

///////////////
 #include<stdio.h>
     int main()
     {
         int x = 10;
         int const *const p = &x;
         //p = &x;
         printf("%d\n", *p);
     }


/////////////////////////
/////Constant pointer//////////////
 #include <stdio.h>
        int x = 0;
        void main()
        {
            int
            *const ptr = &x;
            printf("%p\n", ptr);
            ptr++;   ///read only pointer
            printf("%p\n ", ptr);
        }
        
