#include <stdio.h>
    int main()
          {
          
            int *ptr; 
            int a = 10;
            ptr = &a;
            *ptr = *ptr + 1;
            printf("%d,%d", *ptr, a); 
          }

// ++(*ptr)   ===  value is pre-incremented in pointer
//  ++ptr     ===  Pointer addreess will increment (depentds data type)
